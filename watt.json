{
  "$schema": "https://schemas.platformatic.dev/@platformatic/next/3.10.0.json",
  "cache": {
    "adapter": "valkey",
    "url": "valkey://{PLT_VALKEY_HOST}:6379",
    "prefix": "test",
    "maxTTL": 604800
  },
  "runtime": {
    "logger": {
      "level": "{PLT_SERVER_LOGGER_LEVEL}"
    },
    "server": {
      "hostname": "0.0.0.0",
      "port": "{PORT}"
    },
    "workers": "{PLT_NEXT_WORKERS}",
    "metrics": {
      "port": 9090
    }
  }
}
